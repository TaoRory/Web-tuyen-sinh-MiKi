                            <h2>CHƯƠNG TRÌNH</h2>
                            <div class="container">
                                <div class="row align-items-center justify-between">
                                    <t t-foreach="student_type_ids" t-as="student_type">
                                        <div class="col-6">
                                            
                                            <label t-att-for="student_type.name">
                                            <input type="radio" t-att-id="student_type.name" name="student_type_id"
                                                   t-att-value="student_type.id"
                                                   t-att-checked="'checked' if (student.student_type_id and student_type.id == student.student_type_id.id) else None"
                                                   class="radio-student-type"/>
                                            <t t-out="student_type.name"/></label>
                                        </div>
                                    </t>
                                </div>
                            </div>
                            <h2>NHÓM MÔN HỌC LỰA CHỌN</h2>
                            <t t-foreach="student_type_ids" t-as="student_type">

                                <div t-attf-class="container department {{student_type.id}}"   t-att-hidden="None if (student.student_type_id and student_type.id == student.student_type_id.id) else 'hidden'">
                                    <div class="row align-items-center justify-between">
                                        <t t-foreach="student_type.department_ids" t-as="department">
                                            <div t-attf-class="col-6 ">
                                                <h3><t t-out="department.name"/></h3>
                                                <br/>
                                                <t t-foreach="department.major_ids" t-as="major">   
                                                        <label t-att-for="major.id">
                                                            <input type="radio" t-att-id="major.id" name="major_id"
                                                               class="radio-major"
                                                               required="1"
                                                               t-att-value="major.id"
                                                               t-att-checked="'checked' if (student.major_id and major.id == student.major_id.id) else None"/>
                                                            <t t-out="major.name"/>(<t t-out="'-'.join(major.subject_ids.mapped('name'))"/>)
                                                        </label>
                                                </t>
                                            </div>

                                        </t>

                                    </div>
                                </div>
                            </t>

                            <!-- Add more checkboxes as needed -->
